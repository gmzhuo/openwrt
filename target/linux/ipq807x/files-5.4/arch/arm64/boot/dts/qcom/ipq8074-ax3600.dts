					pa_therm2 {
						reg = <VADC_PMP8074_AMUX_THM2>;
						qcom,vadc-thermal-node;
					};
					pa_therm3 {
						reg = <VADC_PMP8074_AMUX_THM3>;
						qcom,vadc-thermal-node;
					};
					die_temp {
						reg = <VADC_PMP8074_DIE_TEMP>;
					};
				};
			};

			pmic@1 {
				compatible ="qcom,spmi-pmic";
				reg = <0x1 SPMI_USID>;
				#address-cells = <1>;
				#size-cells = <0>;

				regulators {
