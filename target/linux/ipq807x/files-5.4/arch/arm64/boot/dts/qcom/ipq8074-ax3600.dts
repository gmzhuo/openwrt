			qcom,ee = <0>;
			qcom,channel = <0>;
			#address-cells = <2>;
			#size-cells = <0>;
			interrupt-controller;
			#interrupt-cells = <4>;
			cell-index = <0>;

			pmic@0 {
				compatible ="qcom,spmi-pmic";
				reg = <0x0 SPMI_USID>;
				#address-cells = <1>;
				#size-cells = <0>;

				pmp8074_vadc: vadc@3100 {
					compatible = "qcom,pmp8074-spmi-vadc";
					reg = <0x3100 0x100>;
					interrupts = <0x0 0x31 0x0 IRQ_TYPE_EDGE_RISING>;
